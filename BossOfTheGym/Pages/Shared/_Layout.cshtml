 <!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BossOfTheGym</title>
    @await Html.PartialAsync("_StyleValidationPartial")
    @await RenderSectionAsync("Styles", required: false)
</head>
<body>
    <div class="main-site__wrapper d-flex flex-column">
        @await Html.PartialAsync("_HeaderPartial")

        <div class="progressBar"></div>

        <main class="main">
            <div class="body-site__wrapper">
                @RenderBody()
            </div>
        </main>

        @await Html.PartialAsync("_FooterPartial")
    </div>

    @await Html.PartialAsync("_ScriptsValidationPartial")

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>